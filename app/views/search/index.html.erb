<h1>Search#index</h1>
<p>Find me in app/views/search/index.html.erb</p>


<%= form_tag({ controller: :search, action: :search },
  id: :fm, class: :search) do %>
  <label for="keywd">検索キーワード：</label>
  <%= text_field_tag :keywd, '', size: 30 %>
  <%= submit_tag '検索' %>
<% end %>

<div id="result">
<%= @title %>
<%= @price %>
<%= @author %>
<%= @largeimage_url %>
<%= @msg %>
</div>
